    "import os\n",
    "import telebot \n",
    "\n",
    "f = open(\"key.text\", \"r\")\n",
    "key = f.read()\n",
    "f.close()\n",
    "\n",
    "bot = telebot.TeleBot(key)\n",
    "\n",
    "\n",
    "@bot.message_handler(commands=[\"Greet\"])\n",
    "\n",
    "def greet(message):\n",
    "    bot.send_message(message.chat.id, \"Hey! \")\n",
    "    \n",
    "\n",
    "    \n",
    "bot.polling()"
   ]
